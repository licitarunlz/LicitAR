@model PersonaModel

@{
    ViewData["Title"] = "Detalles de Proveedor";
    Layout = "~/Views/Shared/_ContentNavbarLayout.cshtml";
}


<div class="row">
    <div class="col-xl">
        <div class="card mb-6">
            <div class="card-header d-flex justify-content-between align-items-center">
                <h5 class="mb-0"><b>@Html.DisplayFor(model => model.Cuit) </b> - @Html.DisplayFor(model => model.RazonSocial)  </h5>
            </div>
            <div class="card-body">
                <dl class="row">
                    <dt class="col-sm-3">
                        @Html.DisplayNameFor(model => model.Cuit)
                    </dt>
                    <dd class="col-sm-9">
                        @Html.DisplayFor(model => model.Cuit)
                    </dd>
                    <dt class="col-sm-3">
                        @Html.DisplayNameFor(model => model.RazonSocial)
                    </dt>
                    <dd class="col-sm-9">
                        @Html.DisplayFor(model => model.RazonSocial)
                    </dd>
                    <dt class="col-sm-3">
                        @Html.DisplayNameFor(model => model.IdTipoPersona)
                    </dt>
                    <dd class="col-sm-9">
                        @Html.DisplayFor(model => model.TipoPersona)
                    </dd>
                    <h4 class="mt-6"> Contacto  </h4>
                    <hr />
                    <dt class="col-sm-3">
                        @Html.DisplayNameFor(model => model.Email)
                    </dt>
                    <dd class="col-sm-9">
                        @Html.DisplayFor(model => model.Email)
                    </dd>
                    <dt class="col-sm-3">
                        @Html.DisplayNameFor(model => model.Telefono)
                    </dt>
                    <dd class="col-sm-9">
                        @Html.DisplayFor(model => model.Telefono)
                    </dd>

                    <h4 class="mt-6"> Domicilio </h4>
                    <hr />
                    <dt class="col-sm-3">
                        @Html.DisplayNameFor(model => model.IdProvincia)
                    </dt>
                    <dd class="col-sm-9">
                        @Html.DisplayFor(model => model.Provincia)
                    </dd>
                    <dt class="col-sm-3">
                        @Html.DisplayNameFor(model => model.IdLocalidad)
                    </dt>
                    <dd class="col-sm-9">
                        @Html.DisplayFor(model => model.Localidad)
                    </dd>
                    <dt class="col-sm-3">
                        @Html.DisplayNameFor(model => model.DireccionBarrio)
                    </dt>
                    <dd class="col-sm-9">
                        @Html.DisplayFor(model => model.DireccionBarrio)
                    </dd>
                    <dt class="col-sm-3">
                        @Html.DisplayNameFor(model => model.DireccionCalle)
                    </dt>
                    <dd class="col-sm-9">
                        @Html.DisplayFor(model => model.DireccionCalle)
                    </dd>
                    <dt class="col-sm-3">
                        @Html.DisplayNameFor(model => model.DireccionNumero)
                    </dt>
                    <dd class="col-sm-9">
                        @Html.DisplayFor(model => model.DireccionNumero)
                    </dd>
                    <dt class="col-sm-3">
                        @Html.DisplayNameFor(model => model.DireccionPiso)
                    </dt>
                    <dd class="col-sm-9">
                        @Html.DisplayFor(model => model.DireccionPiso)
                    </dd>
                    <dt class="col-sm-3">
                        @Html.DisplayNameFor(model => model.DireccionDepto)
                    </dt>
                    <dd class="col-sm-9">
                        @Html.DisplayFor(model => model.DireccionDepto)
                    </dd>
                    <dt class="col-sm-3">
                        @Html.DisplayNameFor(model => model.DireccionCodigoPostal)
                    </dt>
                    <dd class="col-sm-9">
                        @Html.DisplayFor(model => model.DireccionCodigoPostal)
                    </dd>
                </dl>
            </div>
        </div>
    </div>
</div>

 


<div class="text-start mt-8">
    <a asp-action="Edit" asp-route-id="@Model?.IdPersona" class="btn btn-primary">
        <i class="bx bx-edit"></i>&nbsp;Editar
    </a>
    <a asp-action="Index" class="btn btn-secondary">
        <i class="bx bx-arrow-back"></i>&nbsp;Volver al listado
    </a>
</div>
